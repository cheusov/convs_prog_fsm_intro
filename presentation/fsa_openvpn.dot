digraph finite_state_machine {
    rankdir=LR;
    size="2,5"

    node [shape = doublecircle]; 7;
    node [shape = point ]; qi

    node [shape = circle];
    qi -> 0;
    0  -> 0 [ label = "o|p|e|n|v|p|n|?" ];
    0  -> 1 [ label = "o" ];
    1  -> 2 [ label = "p" ];
    2  -> 3 [ label = "e" ];
    3  -> 4 [ label = "n" ];
    4  -> 5 [ label = "v" ];
    5  -> 6 [ label = "p" ];
    6  -> 7 [ label = "n" ];
}
